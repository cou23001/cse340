<% if (title) { %>
  <div class="mgmt">
    <h1><%= title %></h1>
    <%- messages() %>
    <% if (errors) { %>
      <ul class="notice">
     <% errors.array().forEach(error => { %>
       <li><%= error.msg %></li>
    <%  }) %>
     </ul>
    <% } %>
    <h2>Welcome <%= locals.accountData.account_firstname %></h2>
    <p>You are logged in</p>
    <p><a href="/account/edit/<%= locals.accountData.account_id %>">Edit Account Information</a></p>
  </div>
    <% } else {
      res.redirect('/')
} %>

<%# messages() %>